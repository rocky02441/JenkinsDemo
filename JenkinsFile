pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Hello Checkout'
            }
        }
    }


    stages {
        stage('Build') {
            steps {
                echo 'Hello Build'
            }
        }
    }


    stages {
        stage('Application Test') {
            steps {
                echo 'Hello Test'
            }
        }
    }


    stages {
        stage('Deploy') {
            steps {
                echo 'Hello Deploy'
            }
        }
    }
}
